  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^18.0.3",
    "concurrently": "^7.2.2",
    "nodemon": "^2.0.19",
    "ts-node": "^10.8.2",
    "typescript": "^4.5.4",
    "vite": "^2.9.9"
  },
  "dependencies": {
    "express": "^4.18.1"
  }
}
